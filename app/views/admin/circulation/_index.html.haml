#data-viz
  %h2 Pick Up requests
  .container
    #left-col
      %h3= Circulation::PickUpRequest.format_date_range
      = bar_chart Circulation::PickUpRequest.gather_data, stacked: true
    #right-col
      %h3= Circulation::PickUpRequest.format_date_range(date_range: 5.weeks.ago.beginning_of_week..5.weeks.ago.end_of_week)
      = bar_chart Circulation::PickUpRequest.gather_data(date_range: 5.weeks.ago.beginning_of_week..5.weeks.ago.end_of_week), stacked: true

%h2 Data
%ul
  - resources.each do |resource|
    %li= link_to resource.resource_name.human, "/admin/#{resource.resource_name.param_key.pluralize}"
